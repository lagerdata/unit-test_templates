//-------------------------EXPORTED FUNCTIONS-------------------------------
int main(void)
{
    HAL_Init();
    _systemClockConfig();

    


    UnityBegin("test1.c");
    DO_TEST(test_thatSucceeds);
    DO_TEST(test_succeedsWithCondition);
    UnityEnd();
    while(1);


    return 0;
}

