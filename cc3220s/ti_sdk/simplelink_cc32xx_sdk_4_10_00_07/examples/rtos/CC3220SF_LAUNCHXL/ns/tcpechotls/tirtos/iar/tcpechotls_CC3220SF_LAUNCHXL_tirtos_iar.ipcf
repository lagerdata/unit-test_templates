<?xml version="1.0" encoding="UTF-8"?>
<!-- IAR Project Connection File -->

<iarProjectConnection version="1.2" name="TCP Echo Example with TLS" oneShot="true"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="IarProjectConnection_1.2.xsd">

    <device>
        <name>CC3220SF</name>
    </device>

    <asmIncludePath>
        <path>$PROJ_DIR$</path>
    </asmIncludePath>

    <includePath>
        <path>$PROJ_DIR$</path>
        <path>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/net/bsd</path>
        <path>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source</path>
        <path>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/posix/iar</path>
    </includePath>

    <extraOptions>
        <args>
            <arg>-f$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC3220SF_LAUNCHXL/release/iar/compiler.opt.defs</arg>
            <arg>--debug</arg>
            <arg>--silent</arg>
            <arg>-e</arg>
            <arg>--aeabi</arg>
            <arg>--thumb</arg>
            <arg>--diag_suppress=Pa050</arg>
        </args>
    </extraOptions>

    <linkerFile>
        <override>true</override>
        <path>$PROJ_DIR$/CC3220SF_LAUNCHXL_TIRTOS.icf</path>
    </linkerFile>

    <linkerExtraOptions>
        <args>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/net/lib/iar/m4/slnetsock_release.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/net/sntp/lib/iar/m4/sntp_release.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/net/wifi/slnetif/iar/Release/slnetifwifi.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/display/lib/display.arm4</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/grlib/lib/iar/m4/grlib.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/third_party/spiffs/lib/iar/m4/spiffs.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/lib/drivers_cc32xx.arm4</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/third_party/fatfs/lib/iar/m4/fatfs.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/drivers/net/wifi/iar/rtos/simplelink.a</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/tirtos/packages/ti/dpl/lib/dpl_cc32xx.arm4</arg>
            <arg>-f$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC3220SF_LAUNCHXL/release/iar/linker.cmd</arg>
            <arg>$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/devices/cc32xx/driverlib/iar/Release/driverlib.a</arg>
            <arg>--silent</arg>
            <arg>--cpu=Cortex-M4</arg>
        </args>
    </linkerExtraOptions>

    <customBuildTools>
        <customBuildTool atRoot="true" name="SysConfig"> <!-- Custom build tool for entire project-->
            <fileExtensions>.syscfg</fileExtensions>
            <command>$SYSCONFIG_ROOT$/sysconfig_cli.bat -o $PROJ_DIR$ -s "$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/.metadata/product.json" --compiler iar $FILE_PATH$
            </command>
            <output>
                <path>SysConfig_Generated_Files.ipcf</path>
            </output>
            <hasPrio>true</hasPrio> <!-- true: run this tool before all other tools-->
        </customBuildTool>
    </customBuildTools>

    <heaps>
        <heap> <!-- If omitted, id="0" -->
            <size>0</size>
        </heap>
        <heap id="1">
            <size>0</size>
        </heap>
    </heaps>
    <stacks>
        <stack> <!-- If omitted, id="0" -->
            <size>512</size>
        </stack>
    </stacks>

    <preBuild>
        <command>iarbuild.exe $SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/kernel/tirtos/builds/CC3220SF_LAUNCHXL/release/iar/tirtos_builds_CC3220SF_LAUNCHXL_release.ewp Debug -log all</command>
    </preBuild>

    <postBuild>
        <command>ielftool --bin --verbose $TARGET_BPATH$.out $TARGET_BPATH$.bin</command>
    </postBuild>

    <files>
        <path copyTo="$PROJ_DIR$/platform.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/platform.c</path>
        <path copyTo="$PROJ_DIR$/tcpEchoTLS.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/tcpEchoTLS.c</path>
        <path copyTo="$PROJ_DIR$/README.md">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/README.md</path>
        <path copyTo="$PROJ_DIR$/README.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/README.html</path>
        <path copyTo="$PROJ_DIR$/startsntp.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/startsntp.c</path>
        <path copyTo="$PROJ_DIR$/common.syscfg">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/common.syscfg</path>
        <path copyTo="$PROJ_DIR$/main_tirtos.c">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/tirtos/main_tirtos.c</path>
        <path reference="$PROJ_DIR$/Board.html">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/source/ti/boards/CC3220SF_LAUNCHXL/Board.html</path>
        <path copyTo="$PROJ_DIR$/CC3220SF_LAUNCHXL_TIRTOS.icf">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/tirtos/iar/CC3220SF_LAUNCHXL_TIRTOS.icf</path>
        <group name="certs" build="true">
            <path copyTo="$PROJ_DIR$/certs/certificate.h">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/certs/certificate.h</path>
            <path copyTo="$PROJ_DIR$/certs/caCert.pem">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/certs/caCert.pem</path>
            <path copyTo="$PROJ_DIR$/certs/caKey.pem">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/certs/caKey.pem</path>
            <path copyTo="$PROJ_DIR$/certs/serverCert.pem">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/certs/serverCert.pem</path>
            <path copyTo="$PROJ_DIR$/certs/serverKey.pem">$SIMPLELINK_CC32XX_SDK_INSTALL_DIR$/examples/rtos/CC3220SF_LAUNCHXL/ns/tcpechotls/certs/serverKey.pem</path>
        </group>
    </files>
</iarProjectConnection>